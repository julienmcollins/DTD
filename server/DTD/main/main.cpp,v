#include <iostream>
#include <stdio.h>
#include "Application.h"

int main(int argc, char * argv[]) {
    // Create Application
    Application application;
    
    // Start up SDL and create window
    //Load media
    if (!application.loadMedia()) {
        printf("Failed to load media!\n");
    } else {
        //Apply the image
        application.update();
    }
    
    return 0;
}
